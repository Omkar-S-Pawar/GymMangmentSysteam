@using GSM.Service.ViewModel;
@model IEnumerable<GSM.Service.ViewModel.vwWorkout>
@{
    int id = 1;
  
}
@foreach (var employee in (IEnumerable<vwWorkout>)TempData["Data"])
{
    <div class="col pt-3">
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="@("chk" + id)" value="@id" />
            <label class="form-check-label" for="@("chk" + id)"> @employee.Name</label>
        </div>
    </div>
    id++;
} 